{
    "name": "oyic/ajax-full-screen-search",
    "description": "AJAX-powered full-screen search with CPT filtering and pagination",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Alberto",
            "email": "oyic@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OYIC\\AjaxSearch\\": "includes/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "*": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php -r \"echo 'OYIC Ajax Search: Dependencies installed successfully.\\n';\""
        ],
        "post-update-cmd": [
            "@php -r \"echo 'OYIC Ajax Search: Dependencies updated successfully.\\n';\""
        ]
    }
}